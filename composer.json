{
    "name": "kodewbit/youtube",
    "description": "A small library that acts as a Laravel wrapper for YouTube data API v3.",
    "keywords": ["youtube", "api"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Yanes Batista",
            "email": "leo95batista@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "google/apiclient": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Kodewbit\\YouTube\\": "src/"
        }
    },
    "scripts": {
        "post-update-cmd": "Google\\Task\\Composer::cleanup"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kodewbit\\YouTube\\YouTubeServiceProvider"
            ]
        },
        "google/apiclient-services": [
            "YouTube"
        ]
    }
}
